{
  "name": "webappid/laravel-smartnotify",
  "description": "Smart Notification for laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Dyan Galih",
      "email": "dyan.galih@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "WebAppId\\SmartNotify\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebAppId\\SmartNotify\\Tests\\": "src/tests"
    }
  },
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "5.5.*|6.*|^7.0",
    "guzzlehttp/guzzle": "^6.3",
    "doctrine/dbal": "^2.5",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.2.6",
    "squizlabs/php_codesniffer": "3.*",
    "mockery/mockery": "dev-master@dev",
    "orchestra/testbench": "~3.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
