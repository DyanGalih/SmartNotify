{
  "name": "webappid/laravel-smartnotify",
  "description": "Smart Notification for laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Dyan Galih",
      "email": "dyan.galih@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "WebAppId\\SmartNotify\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebAppId\\SmartNotify\\Tests\\": "src/tests"
    }
  },
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "^7.0|^8.0",
    "guzzlehttp/guzzle": "^6.3",
    "doctrine/dbal": "^2.5",
    "ext-json": "*"
  },
  "require-dev": {
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "WebAppId\\SmartNotify\\ServiceProvider"
      ]
    }
  }
}
